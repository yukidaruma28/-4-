package org.example;

public class main {

  public static void main(String[] args) {
    // 文字列結合と数値計算の結果を結合して
    // 自分の名前 + 自分の年齢 + "歳です。"が出力されるようにする。
    String FullName = "ゆきだるま";
    String wordHA = "は";
    String EndWord = "歳です。";
    int age = 10 + 10 + 8;
    System.out.println(FullName + wordHA + age + EndWord);

  }
}
